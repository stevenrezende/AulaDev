package lacosrepeticao;

public class LacosRepeticao {

	public static void main(String[] args) {
		int x =15 ;
		int m[][] = new int[2][4];

		while (x <18) {
			System.out.println("Você não tem permissao para entrar");
			
			m[1][x] = 6;
			x++;
		}
		while (x <18) {
			System.out.println("Vetores preenchidos" + m[1][x] );
			
			
			x++;
		}

	}

}
